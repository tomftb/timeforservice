{% set backPath=path('app_client_index') %}
{% extends 'modalBase.html.twig' %}
{% block title %}{{ parent() }} - Client cooperation - {{ client.id }}{% endblock %}
{% block body %}
<div class="m-4 p-4 modal:m-0 modal:p-0 bg-gray-800 rounded-lg">
    <div class="flex justify-between">
        <h1 class="text-xl font-semibold text-white mb-4">Client {{ client.name }} terms of cooperation:</h1>
        <div class="mr-0">
            {# CLOSE ACTION - DEPENDS OF WINDOWS TYPE #}
            {{ loadFrame.closeAction | raw }}
            {# END CLOSE ACTION #}
        </div>
    </div>
    {# FORM #}
    {{ include('client/_cooperation_form.html.twig') }}
</div>
{% endblock %}