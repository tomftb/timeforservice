<tr class="even:bg-gray-700 odd:bg-gray-600">
    <td class="px-6 py-4 whitespace-nowrap">{{ client.id }}</td>
    <td class="px-6 py-4 whitespace-nowrap">{{ client.name }}</td>
    <td class="px-6 py-4 whitespace-nowrap">{{ client.nin }}</td>
    <td class="px-6 py-4">{{ client.street }}</td>
    <td class="px-6 py-4 whitespace-nowrap">{{ client.town }}</td>
    <td class="px-6 py-4 whitespace-nowrap">
        <a href="{{ path('app_client_show', {'id': client.id}) }}" class="text-blue-400 hover:text-blue-600">show</a>
        <a href="{{ path('app_client_edit', {'id': client.id}) }}" class="ml-4 text-yellow-400 hover:text-yellow-600">edit</a>
    </td>
</tr>